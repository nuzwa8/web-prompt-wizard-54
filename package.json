{
  "name": "web-prompt-wizard",
  "version": "1.0.0",
  "description": "AI-powered prompt optimization tool with copy, regenerate and structured output features",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "audit": "npm audit",
    "audit-fix": "npm audit fix",
    "setup": "npm install && cp .env.example .env && echo 'Setup complete! Please add your OpenAI API key to .env file'",
    "build": "echo 'No build step required for this project'",
    "clean": "rm -rf node_modules package-lock.json && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "keywords": ["prompt", "optimization", "AI", "wizard", "generator"],
  "author": "MiniMax Agent",
  "license": "MIT"
}